  <project name="python-novaclient"     path="overcast-monolith/python-novaclient" />
  <project name="nova"                  path="overcast-monolith/nova" />

  <project name="python-glanceclient"   path="overcast-monolith/python-glanceclient" />
  <project name="glance"                path="overcast-monolith/glance" />

  <project name="python-neutronclient"  path="overcast-monolith/python-neutronclient" />
  <project name="neutron"               path="overcast-monolith/neutron" />

  <project name="python-ironicclient"   path="overcast-monolith/python-ironicclient" />
  <project name="ironic"                path="overcast-monolith/ironic" />

  <project name="python-swiftclient"    path="overcast-monolith/python-swiftclient" />

  <project name="tempest-lib"           path="overcast-monolith/tempest-lib" />
  <project name="tempest"               path="overcast-monolith/tempest" />

  <project name="horizon"               path="overcast-monolith/horizon" />

  <project name="oslo.messaging"        path="overcast-monolith/oslo.messaging" />
