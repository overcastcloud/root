  <!-- OpenStack stuff -->
  <project name="python-keystoneclient" path="overcast-monolith/python-keystoneclient" />
  <project name="keystone"              path="overcast-monolith/keystone" />

  <project name="python-cinderclient"   path="overcast-monolith/python-cinderclient" />
