  <project name="oslo.log"              path="overcast-monolith/oslo.log" />
  <project name="keystonemiddleware"    path="overcast-monolith/keystonemiddleware" />

  <!-- Misc dependencies -->
  <project name="pbr"                   path="overcast-monolith/pbr" />
  <project name="python-six"            path="overcast-monolith/python-six" />
  <project name="websockify"            path="overcast-monolith/websockify" />
